<div class="row">
    <% athletes.forEach (function(model) { %>

    <div class="card col-12 col-sm-12 col-md-6">


            <div class="media-body col-12">

                <div class="card" style="width: 18rem;">
                    <ul class="list-group">

                        <li class="list-group-item">Ahtlete Name: <%= model.athleteName %></li>
                        <li class="list-group-item">Ahtlete ID: <%= model.athleteID %></li>
                        <li class="list-group-item">Difficult Score: <%= model.dScore %></li>
                        <li class="list-group-item">Execution Score: <%= model.eScore %></li>
                        <li class="list-group-item">Total Score: <%= model.totalScore %></li>

                    </ul>
                </div>

                <p class="mb-0"></p>
            </div>
    
    </div>
    <%}); %>
</div>
<br><br>