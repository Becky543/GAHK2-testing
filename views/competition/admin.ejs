<div class="container">
    <div class="row">
      <!-- The 12 cards -->
      <div class="col-12">
        <table class="table">
  
          <thead>
            <tr>
              <th scope="col">Sequence</th>
              <th scope="col">Athlete Name</th>
              <th scope="col">Competition Event</th>
              <th scope="col">E1</th>
              <th scope="col">E2</th>
              <th scope="col">E3</th>
              <th scope="col">E4</th>
              <th scope="col">E5</th>
              <th scope="col">E6</th>
              <th scope="col">D2</th>
              <th scope="col">Edit</th>
            </tr>
          </thead>
          
          <tbody>
            <% competition.forEach (function(model) { %>
            <tr>
              <th scope="row"><%= model.athleteID %></th>
             
              <td><%= model.athleteName %></td>
              <td><%= model.competitionEvent %></td>
              <td><%= model.e1Score %></td>
              <td><%= model.e2Score %></td>
              <td><%= model.e3Score %></td>
              <td><%= model.e4Score %></td>
              <td><%= model.e5Score %></td>
              <td><%= model.e6Score %></td>
              <td><%= model.d2Score %></td>

              <td><a href="/competition/updateE2/<%= competition.id %>">Edit</a></td>

            </tr>
            <%}); %>
  
          </tbody>
        </table>
  
      </div>
  
  
  
  
    </div>
  </div>
  
  
  
  
  
  
  
  